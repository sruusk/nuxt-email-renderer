{
  "name": "nuxt-email-renderer",
  "version": "2.5.2",
  "description": "Nuxt email module for rendering HTML emails with Vue components",
  "repository": "mokkapps/nuxt-email-renderer",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./components": {
      "types": "./dist/runtime/components/components.d.ts"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build && npm run client:build",
    "prepack": "pnpm build",
    "client:build": "nuxi generate client",
    "client:dev": "nuxi dev client --port 3300",
    "dev": "npm run play:dev",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare && nuxi prepare client",
    "docs:dev": "cd docs && pnpm dev",
    "docs:build": "cd docs && pnpm build",
    "preinstall": "node scripts/preinstall.js || echo",
    "prepare": "node scripts/prepare.js",
    "play:dev": "nuxi dev playground",
    "play:prod": "npm run prepack && nuxi dev playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release --patch && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^3.1.1",
    "@nuxt/kit": "^4.3.0",
    "@vitejs/plugin-vue": "^6.0.4",
    "html-to-text": "^9.0.5",
    "js-beautify": "^1.15.4",
    "marked": "7.0.4",
    "shiki": "^3.22.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/devtools-ui-kit": "^3.1.1",
    "@nuxt/eslint-config": "^1.13.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.3.0",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxtjs/i18n": "^10.2.1",
    "@types/html-to-text": "^9.0.4",
    "@types/js-beautify": "^1.14.3",
    "@types/node": "^25.2.0",
    "@vitest/coverage-v8": "3.2.4",
    "changelogen": "^0.6.2",
    "eslint": "^9.39.2",
    "happy-dom": "^20.5.0",
    "jsdom": "^28.0.0",
    "nuxt": "4.1.2",
    "playwright-core": "^1.58.1",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vue-i18n": "^11.2.8",
    "vue-tsc": "^3.2.4"
  },
  "peerDependencies": {
    "@nuxtjs/i18n": ">=10.0.0",
    "vue-i18n": ">=11.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxtjs/i18n": {
      "optional": true
    },
    "vue-i18n": {
      "optional": true
    }
  },
  "resolutions": {
    "nuxt": "^4.1.2",
    "typescript": "^5.9.2",
    "vite": "~7.0.6"
  },
  "packageManager": "pnpm@10.28.2"
}
